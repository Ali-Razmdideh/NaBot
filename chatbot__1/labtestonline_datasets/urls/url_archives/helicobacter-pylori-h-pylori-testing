<section id="block-mainpagecontent" class="block-mainpagecontent">
  
  
    

  
          <div class="block-content-wrapper">
        <div data-history-node-id="1159" class="node node--type-test node--view-mode-full ds-1col clearfix">

  

  <div class="field-wrapper field field-node--field-test-synonyms field-name-field-test-synonyms field-type-string field-label-inline clearfix">
      <div class="field-label">Also Known As</div>
    <div class="field-items">
          <div class="field-item"><em>H. pylori</em> stool antigen</div>
          <div class="field-item"><em>H. pylori</em> breath tests</div>
          <div class="field-item">Urea breath test</div>
          <div class="field-item">CLO test</div>
          <div class="field-item">Rapid urease test (RUT) for <em>H. pylori</em></div>
      </div>
</div>

