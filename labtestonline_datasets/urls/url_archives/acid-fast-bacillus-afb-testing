<section id="block-mainpagecontent" class="block-mainpagecontent">
  
  
    

  
          <div class="block-content-wrapper">
        <div data-history-node-id="1032" class="node node--type-test node--view-mode-full ds-1col clearfix">

  

  <div class="field-wrapper field field-node--field-test-synonyms field-name-field-test-synonyms field-type-string field-label-inline clearfix">
      <div class="field-label">Also Known As</div>
    <div class="field-items">
          <div class="field-item">AFB Smear and Culture</div>
          <div class="field-item">TB Culture and Sensitivity</div>
          <div class="field-item">Mycobacteria Smear and Culture</div>
          <div class="field-item">TB NAAT</div>
          <div class="field-item">Acid-Fast Bacillus Smear and Culture and Sensitivity</div>
